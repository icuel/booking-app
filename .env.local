HUBSPOT_TICKET_PIPELINE_ID=0

# hs_pipeline_stage の中の「予約未確定」の ID
HUBSPOT_TICKET_STAGE_PENDING=1

# カスタムプロパティの内部名
HUBSPOT_SUBJECT_TARGET_TYPE_PROP=subject_target_type
HUBSPOT_SUBJECT_AGE_BAND_PROP=subject_age_band
HUBSPOT_KANA_FULL_NAME_PROP=kana_full_name
